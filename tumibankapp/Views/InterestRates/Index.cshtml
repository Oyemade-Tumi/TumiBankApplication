@model IEnumerable<tumibankapp.Data.InterestRate>

@{
    ViewData["Title"] = "Index";
}

<h1>All Interest Rate</h1>

<p>
    <a asp-action="Create">Create New Interest Rate</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CurrentRate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApplyInterest)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CurrentRate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApplyInterest)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InterestRateId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InterestRateId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InterestRateId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
